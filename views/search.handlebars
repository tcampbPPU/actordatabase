function littleAnimation(elt,idx){
  var colors = ["white","yellow","grey","green"];
  $(elt).animate({
    backgroundColor:colors[idx]
  },300,function(){
    if(idx ===0){
      return;
    }else {
      littleAnimation(elt,idx-1);
    }
  });
}
